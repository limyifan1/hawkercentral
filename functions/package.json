    "node-fetch": "2.6.1",
    "request": "2.88.2",
    "telegraf": "3.38.0",
    "twilio": "3.45.0"
  },
  "devDependencies": {
    "@firebase/testing": "0.20.0",
    "eslint": "6.8.0",
    "eslint-plugin-promise": "4.2.1",
    "firebase-functions-test": "0.2.1",
    "mocha": "7.2.0"
  },
  "private": true
}
